sort(arr,arr+n);
        // int cnt=0;
        int i=0;
        int j=n-1;
        while(i<=j){
            if(arr[i]*arr[j]==x){
            return true;
            }
            else if(arr[i]*arr[j]<x){
                i++;
                
            }
            else if(arr[i]*arr[j]>x){
                j--;
                
            }
            if(arr[i]<0)return true;
             
                  
        }
        return false;
